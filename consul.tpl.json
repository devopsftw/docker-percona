{
    "data_dir": "/tmp/consul",
    "datacenter": "${CONSUL_DC}",
    "ports": {
        "dns": -1
    },
    "services": [
        {
            "name": "${CONSUL_SERVICE_NAME}",
            "port": 3376,
            "checks": [
                {
                "tcp": "localhost:3376",
                "interval": "10s",
                "timeout": "1s"
                }
            ]
        }
    ]
}
